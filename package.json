{
    "name": "gendhingku-server",
    "version": "1.0.0",
    "description": "Backend for Gendhingku",
    "scripts": {
        "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
        "prod": "pm2-runtime build/app.js",
        "test": "jest",
        "build-server": "tsc -p tsconfig-build.json && rm -rf node_modules && npm i --omit=dev",
        "build-client": "cd client && npm i && npm run build && rm -rf node_modules && npm i --omit=dev",
        "build": "npm run build-server && npm run build-client && npm prune --production",
        "start": "node build/app.js"
    },
    "keywords": [],
    "author": "Bima Jikope",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.21.4",
        "bcrypt": "^5.1.0",
        "connect-history-api-fallback": "^1.6.0",
        "connect-redis": "^6.0.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^15.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "mongoose": "^6.0.7",
        "pm2": "^5.2.2",
        "redis": "^3.1.2"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/bcrypt": "^5.0.0",
        "@types/connect-history-api-fallback": "^1.3.5",
        "@types/connect-redis": "^0.0.17",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^27.0.2",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^16.10.1",
        "@types/redis": "^2.8.32",
        "jest": "^27.2.2",
        "nodemon": "^2.0.13",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.3"
    }
}
